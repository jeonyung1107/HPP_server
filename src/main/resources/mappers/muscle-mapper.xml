<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="muscle">
    <insert id="insert" parameterType="muscle">
      INSERT INTO muscle(id, name, parent)
      VALUES (#{id}, #{name}, #{parent})
    </insert>

    <select id="get" resultType="muscle">
      SELECT * FROM muscle WHERE id = #{id}
    </select>

    <delete id="delete">
      DELETE FROM muscle WHERE id = #{id}
    </delete>
</mapper>