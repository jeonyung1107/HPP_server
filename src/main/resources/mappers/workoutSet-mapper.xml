<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="workoutSet">
    <insert id="insert" parameterType="workoutSet">
        <![CDATA[
          INSERT INTO excercise_set(id, session, excercise, intensity, numberof)
          VALUES (#{id}, #{session}, #{excercise}, #{intensity}, #{numberof})
        ]]>
    </insert>

    <select id="get" resultType="workoutSet">
        <![CDATA[
          SELECT * FROM excercise_set WHERE id = #{id}
        ]]>
    </select>

    <delete id="delete">
        <![CDATA[
          DELETE FROM excercise_set WHERE id = #{id}
        ]]>
    </delete>
</mapper>