<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="usersession">
    <insert id="insert" parameterType="usersession">
        <![CDATA[
          INSERT INTO user_session(user, session)
          VALUES (#{user},#{session})
        ]]>
    </insert>

    <select id="get" resultType="usersession">
        <![CDATA[
          SELECT * FROM user_session WHERE id = #{name}
        ]]>
    </select>

    <delete id="delete">
        <![CDATA[
          DELETE FROM user_session WHERE id = #{id}
        ]]>
    </delete>
</mapper>